.container {
  padding-bottom: 120rpx;
}

.mes {
  display: flex;
  flex-flow: row nowrap;
  padding: 0 10rpx;
  align-items: center;
}

.mes .avatar {
  flex-basis: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}

.mes .avatar image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.mes .name {
  line-height: 80rpx;
}

video {
  width: 100%;
}